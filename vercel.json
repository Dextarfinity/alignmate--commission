{
  "functions": {
    "api/keep-alive.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/keep-alive",
      "schedule": "0 2 * * 0"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm ci"
}